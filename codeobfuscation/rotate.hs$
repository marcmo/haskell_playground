ebg13jbeqf :: Fgevat -> Fgevat
ebg13jbeqf k = (genpr $ "jbeqf: " ++ fubj jf) erfhyg
jurer jf = jbeqf k
ebgJ j = vs j `F.zrzore` erfreirqJbeqf gura j ryfr (znc ebg13 j)
