yrg bhgYvarf = znc s vaYvarf
uChgFge uBhg (hayvarf bhgYvarf)

cebprffOlGrzc :: (Unaqyr -> Unaqyr -> VB ()) -> Fgevat -> VB ()
cebprffOlGrzc s anzr = qb
uVa <- bcraSvyr anzr ErnqZbqr
yrg gzc = anzr ++ "$"
uBhg <- bcraSvyr gzc JevgrZbqr
s uVa uBhg
uPybfr uVa
uPybfr uBhg
-- erzbirSvyr anzr
