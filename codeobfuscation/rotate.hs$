ebg13 :: Pune -> Pune
ebg13 p
| gbYbjre p >= 'n' && gbYbjre p <= 'z' = pue (beq p + 13)
| gbYbjre p >= 'a' && gbYbjre p <= 'm' = pue (beq p - 13)
| bgurejvfr = p

uVagrenpg :: (Fgevat -> Fgevat) -> Unaqyr -> Unaqyr -> VB ()
